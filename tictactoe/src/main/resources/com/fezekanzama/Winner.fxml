<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import java.net.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fezekanzama.WinnerController">
    <stylesheets>
      <URL value="@styles.css" />
   </stylesheets>
    <children>
        <Label text="The Winner is..." />
        <Label fx:id="winnerName" styleClass="currentPlayer"/>
        <Button fx:id="playOn" text="Keep Playing" onAction="#keepPlaying" />
        <Button fx:id="EndGame" text="End Game" onAction="#endGame" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
